# Void Reactor Environment Configuration
# Copy this file to .env.local and fill in the values

# Application
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_NAME=The Void Reactor

# Analytics (optional - uncomment and add your keys)
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
# NEXT_PUBLIC_POSTHOG_KEY=

# Monitoring (optional - uncomment and add your keys)
# SENTRY_ORG=
# SENTRY_PROJECT=
# SENTRY_AUTH_TOKEN=
# NEXT_PUBLIC_SENTRY_DSN=

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_SHOW_FPS_MONITOR=false
